{
  "dashboard": {
    "id": null,
    "title": "MLOps Dashboard",
    "tags": ["mlops", "ipl"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Model Predictions",
        "type": "stat",
        "targets": [
          {
            "expr": "ipl_predictions_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list"
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Prediction Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ipl_prediction_latency_seconds_sum[5m]) / rate(ipl_prediction_latency_seconds_count[5m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear"
            },
            "unit": "s"
          }
        }
      },
      {
        "id": 3,
        "title": "Model Load Time",
        "type": "stat",
        "targets": [
          {
            "expr": "ipl_model_load_time_seconds",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list"
            },
            "unit": "s"
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s"
  }
}